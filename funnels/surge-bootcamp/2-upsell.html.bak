  <!-- Footer -->
  <footer class="py-8 px-6 border-t border-slate-800">
    <div class="max-w-4xl mx-auto text-center">
      <p class="text-slate-500 text-sm">
        © 2026 GovCon Giants. All rights reserved.
      </p>
    </div>
